    "#1\n",
    "# from prompt.prompt import MANAGERIAL_ASSESSMENT_PROMPT\n",
    "\n",
    "def assess_answer(\n",
    "    jabatan_name: str,\n",
    "    kompetensi_name: str,\n",
    "    soal_id: str,\n",
    "    jawaban_peserta: str,\n",
    "    nama_peserta: str = \"Peserta Demo\",\n",
    ") -> str:\n",
    "    \"\"\"\n",
    "    Menilai jawaban peserta berdasarkan:\n",
    "    - jabatan\n",
    "    - kompetensi\n",
    "    - soal (diambil dari QUESTIONS_DATA via soal_id)\n",
    "    Menggunakan MANAGERIAL_ASSESSMENT_PROMPT + LLM.\n",
    "    \"\"\"\n",
    "    # Validasi jabatan & kompetensi\n",
    "    if jabatan_name not in SKJ_DATA:\n",
    "        raise ValueError(f\"Jabatan '{jabatan_name}' tidak dikenal.\")\n",
    "\n",
    "    skj = SKJ_DATA[jabatan_name]\n",
    "    if kompetensi_name not in skj[\"kompetensi\"]:\n",
    "        raise ValueError(f\"Kompetensi '{kompetensi_name}' tidak ada di jabatan '{jabatan_name}'.\")\n",
    "\n",
    "    # Ambil level target & deskripsi\n",
    "    komp = skj[\"kompetensi\"][kompetensi_name]\n",
    "\n",
    "    # Ambil soal dari QUESTIONS_DATA\n",
    "    try:\n",
    "        soal_list = QUESTIONS_DATA[jabatan_name][kompetensi_name]\n",
    "    except KeyError:\n",
    "        raise ValueError(f\"Tidak ada soal untuk jabatan '{jabatan_name}', kompetensi '{kompetensi_name}'.\")\n",
    "\n",
    "    try:\n",
    "        soal_obj = next(s for s in soal_list if s[\"id_soal\"] == soal_id)\n",
    "    except StopIteration:\n",
    "        raise ValueError(f\"Soal dengan id '{soal_id}' tidak ditemukan untuk kombinasi tersebut.\")\n",
    "\n",
    "    soal_text = soal_obj[\"teks\"]\n",
    "\n",
    "    # Context untuk LLM (ringkasan SKJ)\n",
    "    context_obj = {\n",
    "        \"jabatan\": jabatan_name,\n",
    "        \"kompetensi\": kompetensi_name,\n",
    "        \"deskripsi\": komp[\"deskripsi\"],\n",
    "        \"level_target\": komp[\"level_target\"],\n",
    "    }\n",
    "    context_text = json.dumps(context_obj, ensure_ascii=False)\n",
    "\n",
    "    # Chain LangChain: PromptTemplate -> LLM\n",
    "    chain = MANAGERIAL_ASSESSMENT_PROMPT | llm\n",
    "\n",
    "    result = chain.invoke(\n",
    "        {\n",
    "            \"context\": context_text,\n",
    "            \"nama\": nama_peserta,\n",
    "            \"jabatan\": jabatan_name,\n",
    "            \"jawaban\": jawaban_peserta,\n",
    "            \"kompetensi\": kompetensi_name,\n",
    "            \"level_target\": str(komp[\"level_target\"]),\n",
    "            \"soal\": soal_text,\n",
    "        }\n",
    "    )\n",
    "\n",
    "    return result.content\n",
    "\n",
    "\n",
    "jabatan = \"Administrator\"\n",
    "kompetensi = \"Integritas\"\n",
    "\n",
    "# ambil id_soal dari QUESTIONS_DATA\n",
    "soal_pilihan = QUESTIONS_DATA[jabatan][kompetensi][0]\n",
    "print(\"Soal yang dipakai:\", soal_pilihan[\"id_soal\"], \"-\", soal_pilihan[\"teks\"])\n",
    "\n",
    "jawaban = \"Saya pernah diminta mengubah data oleh rekan kerja, namun saya menolak dan menjelaskan bahwa hal tersebut melanggar aturan.\"\n",
    "\n",
    "hasil = assess_answer(\n",
    "    jabatan_name=jabatan,\n",
    "    kompetensi_name=kompetensi,\n",
    "    soal_id=soal_pilihan[\"id_soal\"],\n",
    "    jawaban_peserta=jawaban,\n",
    ")\n",
    "\n",
    "print(\"=== HASIL PENILAIAN ===\")\n",
    "print(hasil)\n",
    "\n"
   ]
