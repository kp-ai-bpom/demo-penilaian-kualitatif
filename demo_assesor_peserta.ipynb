    "from typing import Any\n",
    "def _join_docs(docs) -> str:\n",
    "    return \"\\n\\n---\\n\\n\".join(d.page_content for d in docs)\n",
    "\n",
    "\n",
    "def assess_answer_rag(\n",
    "    jabatan_name: str,\n",
    "    kompetensi_name: str,\n",
    "    soal_id: str,\n",
    "    jawaban_peserta: str,\n",
    "    nama_peserta: str = \"Peserta Demo\",\n",
    "    permenpan_retriever: Any = None,\n",
    "    skj_retriever: Any = None,\n",
    ") -> str:\n",
    "    \"\"\"\n",
    "    Menilai jawaban peserta berbasis RAG:\n",
    "    - konteks_permenpan dari retriever PermenPAN\n",
    "    - konteks_skj dari retriever SKJ\n",
    "    - level_target & info dasar diambil dari SKJ_DATA\n",
    "    - soal diambil dari QUESTIONS_DATA\n",
    "    \"\"\"\n",
    "    # Validasi jabatan & kompetensi\n",
    "    if jabatan_name not in SKJ_DATA:\n",
    "        raise ValueError(f\"Jabatan '{jabatan_name}' tidak dikenal.\")\n",
    "\n",
    "    skj = SKJ_DATA[jabatan_name]\n",
    "    if kompetensi_name not in skj[\"kompetensi\"]:\n",
    "        raise ValueError(f\"Kompetensi '{kompetensi_name}' tidak ada di jabatan '{jabatan_name}'.\")\n",
    "\n",
    "    komp = skj[\"kompetensi\"][kompetensi_name]\n",
    "    level_target = komp[\"level_target\"]\n",
    "\n",
    "    # Ambil soal dari QUESTIONS_DATA\n",
    "    try:\n",
    "        soal_list = QUESTIONS_DATA[jabatan_name][kompetensi_name]\n",
    "    except KeyError:\n",
    "        raise ValueError(f\"Tidak ada soal untuk jabatan '{jabatan_name}', kompetensi '{kompetensi_name}'.\")\n",
    "\n",
    "    try:\n",
    "        soal_obj = next(s for s in soal_list if s[\"id_soal\"] == soal_id)\n",
    "    except StopIteration:\n",
    "        raise ValueError(f\"Soal dengan id '{soal_id}' tidak ditemukan untuk kombinasi tersebut.\")\n",
    "\n",
    "    soal_text = soal_obj[\"teks\"]\n",
    "\n",
    "    # Bangun query untuk RAG\n",
    "    query = f\"Jabatan: {jabatan_name}. Kompetensi: {kompetensi_name}. Soal: {soal_text}. Jawaban: {jawaban_peserta}.\"\n",
    "\n",
    "    # Ambil konteks PermenPAN\n",
    "    context_permenpan = \"\"\n",
    "    if permenpan_retriever is not None:\n",
    "        docs_p = permenpan_retriever.get_relevant_documents(query)\n",
    "        context_permenpan = _join_docs(docs_p)\n",
    "\n",
    "    # Ambil konteks SKJ\n",
    "    context_skj = \"\"\n",
    "    if skj_retriever is not None:\n",
    "        docs_s = skj_retriever.get_relevant_documents(query)\n",
    "        context_skj = _join_docs(docs_s)\n",
    "\n",
    "    # Safety fallback kalau konteks kosong\n",
    "    if not context_permenpan and not context_skj:\n",
    "        # minimal isi ringkasan dari SKJ_DATA\n",
    "        context_skj = json.dumps(\n",
    "            {\n",
    "                \"jabatan\": jabatan_name,\n",
    "                \"kompetensi\": kompetensi_name,\n",
    "                \"deskripsi\": komp[\"deskripsi\"],\n",
    "                \"level_target\": level_target,\n",
    "            },\n",
    "            ensure_ascii=False,\n",
    "        )\n",
    "\n",
    "    chain = MANAGERIAL_ASSESSMENT_PROMPT | llm\n",
    "\n",
    "    result = chain.invoke(\n",
    "        {\n",
    "            \"context_permenpan\": context_permenpan,\n",
    "            \"context_skj\": context_skj,\n",
    "            \"nama\": nama_peserta,\n",
    "            \"jabatan\": jabatan_name,\n",
    "            \"kompetensi\": kompetensi_name,\n",
    "            \"level_target\": str(level_target),\n",
    "            \"soal\": soal_text,\n",
    "            \"jawaban\": jawaban_peserta,\n",
    "        }\n",
    "    )\n",
    "\n",
    "    return result.content"
   ]
